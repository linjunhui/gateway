	byte locked;
	byte[] gateID = new byte[13];
	byte online;
	Attr[] attrs = new Attr[10];
	//填充用
	byte[] next = new byte[4];
	public byte[] conversbytes() {
		bytes 
		
		return gateID;
		
	}

}

//
 class Attr implements Serializable {
	//13字节
	byte[] attrName = new byte[13];
	//1 字节
	byte dataproperty;
	//4 字节
	int data;
	int times;
}
 
 